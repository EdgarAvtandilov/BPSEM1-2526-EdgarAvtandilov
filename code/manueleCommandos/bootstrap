conf t
hostname toestel
ip domain-name dns.local
no crypto key generate rsa
crypto key zeroize rsa
yes
ip ssh version 2
crypto key generate rsa modulus 2048
username ansible privilege 15 secret Ansible123
line vty 0 4
login local
transport input ssh
end
write mem

per edge router:
interface g0/0
ip address 10.10.x0.2 255.255.255.252
no shutdown
exit
ip route 192.168.72.0 255.255.255.0 10.10.20.1
exit